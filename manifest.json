{
  "manifest_version": 3,
  "name": "ADA Compliance Checker Prototype",
  "version": "0.1",
  "permissions": ["activeTab", "scripting", "contextMenus", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://*/*"
  ]
}
